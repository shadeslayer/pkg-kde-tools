-DCMAKE_BUILD_TYPE=Debian
-DKDE4_BUILD_TESTS=false
-DKDE_DISTRIBUTION_TEXT="Debian packages"
-DCMAKE_SKIP_RPATH:BOOL=OFF
-DKDE4_USE_ALWAYS_FULL_RPATH=false
